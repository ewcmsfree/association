	</parameter>
	<queryString>
		<![CDATA[SELECT t1.id,acad_office, birthday, create_time, duties, fax,
       goreign_level, allowance, case is_bodao when true then '博导' ELSE '' END AS is_bodao,
       case is_shuodao when true then '硕导' ELSE '' END AS is_shuodao, job_resume, job_status,
       learning_situation, located, t1.name, now_professional, office_phone,
       political, postal_address, case sex when 'male' then '男' ELSE '女' END AS sex, situation_resume, specialty,
       t1.status, titles, t1.user_id, work_address, zip_code, nation_id,
       t4.name AS nation_name,t5.email,t5.mobile_phone_number,
       ARRAY_TO_STRING(array((SELECT name FROM sec_organization AS t3,pel_archive_organization AS t2
       WHERE t2.archive_id=t1.id AND t2.organization_id=t3.id)),',') AS organization_name,
       ARRAY_TO_STRING(array((SELECT name FROM pel_archive_current_state AS t7,pel_current_state AS t8
       WHERE t7.currentstate_id = t8.id AND t7.archive_id=t1.id)),',') AS currentstate_name,
       ARRAY_TO_STRING(array((SELECT name FROM pel_archive_allowance AS t9,pel_allowance AS t10
       WHERE t9.allowance_id=t10.id AND t9.archive_id=t1.id)),',') AS allowance_name,
       ARRAY_TO_STRING(array(SELECT university_name FROM pel_education_experience AS t11 WHERE t11.user_id=t1.user_id),'@') AS university_name,
       ARRAY_TO_STRING(array(SELECT major FROM pel_education_experience AS t12 WHERE t12.user_id=t1.user_id),'@') AS major,
       ARRAY_TO_STRING(array(SELECT graduation_date FROM pel_education_experience AS t13 WHERE t13.user_id=t1.user_id),'@') AS graduation_date,
       ARRAY_TO_STRING(array(SELECT education FROM pel_education_experience AS t14 WHERE t14.user_id=t1.user_id),'@') AS education,
       ARRAY_TO_STRING(array(SELECT degree FROM pel_education_experience AS t15 WHERE t15.user_id=t1.user_id),'@') AS degree
  FROM pel_archive AS t1,pel_nation AS t4,sec_user AS t5
  WHERE  t1.nation_id=t4.id AND t1.user_id=t5.id
  AND t1.user_id =$P{userId}]]>
	</queryString>
